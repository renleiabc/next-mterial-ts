{
  "extends": ["plugin:@next/next/recommended","next/core-web-vitals","prettier"],
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "parser": "@typescript-eslint/parser"
  },
  "plugins": ["prettier", "react-hooks"], //这个是为了整合我们自己配置.prettierrc.js
  "rules": {
    "prettier/prettier":"error",
    "no-unused-vars":"error"
  }
}
